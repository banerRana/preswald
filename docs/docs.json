          {
            "group": "Data",
            "pages": [
              "data/get_df",
              "data/query"
            ]
          },
          {
            "group": "Layout",
            "pages": [
              "layout/sidebar",
              "layout/topbar",
              "layout/sizing"
            ]
          },
          {
            "group": "Workflow",
            "pages": [
              "workflow/retrypolicy",
              "workflow/workflow",
              "workflow/workflow_analyzer",
              "workflow/workflow_dag"
            ]
          },
          {
            "group": "Deprecated",
            "pages": [
              "deprecated/alert",
              "deprecated/button",
              "deprecated/chat",
              "deprecated/playground",
              "deprecated/progress",
              "deprecated/separator",
              "deprecated/spinner"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://join.slack.com/t/structuredlabs-users/shared_invite/zt-33zwhyv3l-6Xu4bHL6b6~bI3z9fvlUig",
          "icon": "slack"
        },
        {
