{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Preswald",
  "colors": {
    "primary": "#000000",
    "light": "#000000",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "configuration"
            ]
          },
          {
            "group": "CLI Reference",
            "pages": [
              "cli/tutorial",
              "cli/init",
              "cli/run",
              "cli/export"
            ]
          },
          {
            "group": "Usage",
            "pages": [
              "usage/examples",
              "usage/troubleshooting"
            ]
          }
        ]
      },
      {
        "tab": "SDK Reference",
        "groups": [
          {
            "group": "Displays",
            "pages": [
              "displays/text",
              "displays/table",
              "displays/matplotlib",
              "displays/plotly",
              "displays/image"
            ]
          },
          {
            "group": "Controls",
            "pages": [
              "controls/slider",
              "controls/selectbox",
              "controls/checkbox",
              "controls/text_input"
            ]
